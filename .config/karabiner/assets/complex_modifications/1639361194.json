{
  "title": "Prevent unintended command-h hide window (rev 3)",
  "rules": [
    {
      "description": "Disable Cmd+H Hide (rev 3)",
      "manipulators": [
        {
          "type": "basic",
          "description": "",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["command"]
            }
          },
          "to": {
            "key_code": "H",
            "modifiers":
              ["left_shift", "command"]
          }
        }
      ]
    }
  ]
}
